{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "env": {
        "compile": {
          "presets": [
            "@babel/preset-react"
          ]
        }
      },
      "plugins": [
        [
          "@babel/plugin-proposal-class-properties",
          {
            "loose": false
          }
        ],
        [
          "module-resolver",
          {
            "root": [
              "src"
            ]
    }]]
}